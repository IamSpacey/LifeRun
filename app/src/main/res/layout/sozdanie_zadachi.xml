<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E6ECFE">

    <Button
        android:id="@+id/button"
        android:layout_width="254dp"
        android:layout_height="52dp"
        android:autoText="false"
        android:background="@drawable/login_btn"
        android:backgroundTint="@android:color/transparent"
        android:backgroundTintMode="multiply"
        android:hapticFeedbackEnabled="false"
        android:radius="70dp"
        android:text="Назад"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#FFFFFF"
        android:textSize="22sp"
        app:rippleColor="#00C32D2D"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="695dp" />

    <ImageView
        android:id="@+id/imageView19"
        tools:layout_editor_absoluteX="287dp"
        android:layout_width="77dp"
        android:layout_height="66dp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/battary_logo" />

    <TextView
        android:id="@+id/battary_stats7"
        android:layout_width="51dp"
        android:layout_height="27dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="16dp"
        android:text="0%"
        android:textAlignment="center"
        android:textColor="#343434"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView51"
        tools:layout_editor_absoluteX="206dp"
        tools:layout_editor_absoluteY="43dp"
        android:layout_width="62dp"
        android:layout_height="25dp"
        android:text="XX.XX"
        android:textAlignment="center"
        android:textColor="#343434"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/textView52"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="43dp"
        android:layout_width="64dp"
        android:layout_height="25dp"
        android:text="XXXX"
        android:textAlignment="center"
        android:textColor="#343434"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/textView53"
        android:layout_width="126dp"
        android:layout_height="27dp"
        android:layout_marginTop="16dp"
        android:text="Имя"
        android:textAlignment="center"
        android:textColor="#343434"
        android:textSize="20sp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="142dp" />

    <TextView
        android:id="@+id/textView55"
        android:layout_width="350dp"
        android:layout_height="115dp"
        android:layout_marginTop="156dp"
        android:text="Текст задачи"
        android:textAlignment="textStart"
        android:textColor="#343434"
        android:textSize="20sp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="27dp" />

    <TextView
        android:id="@+id/textView54"
        android:layout_width="291dp"
        android:layout_height="45dp"
        android:layout_marginTop="104dp"
        android:text="Название задачи"
        android:textAlignment="textStart"
        android:textColor="#343434"
        android:textSize="28sp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="27dp" />

    <TextView
        android:id="@+id/textView56"
        android:layout_width="109dp"
        android:layout_height="23dp"
        android:layout_marginTop="280dp"
        android:text="Редактировать"
        android:textAlignment="textStart"
        android:textColor="#494949"
        android:textSize="14sp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="27dp" />

    <ImageView
        android:id="@+id/imageView20"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="20dp"
        android:layout_width="44dp"
        android:layout_height="43dp"
        app:srcCompat="@drawable/settings_logo" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottom_navigation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="790dp"
        android:background="#3B708D"
        android:textColor="#FFFFFF"
        app:layout_constraintTop_toTopOf="parent"
        app:menu="@menu/bottom_nav_menu" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="58dp"
        android:layout_height="52dp"
        app:srcCompat="@drawable/kolokolchik"
        tools:layout_editor_absoluteX="328dp"
        tools:layout_editor_absoluteY="695dp" />

</androidx.constraintlayout.widget.ConstraintLayout>